-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 
LSS += \
ADC_DRIV.lss \

FLASH_IMAGE += \
ADC_DRIV.hex \

SIZEDUMMY += \
sizedummy \


# All Target
all: ADC_DRIV.elf secondary-outputs

# Tool invocations
ADC_DRIV.elf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: AVR C Linker'
	avr-gcc -Wl,-Map,ADC_DRIV.map -mmcu=atmega32 -o "ADC_DRIV.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

ADC_DRIV.lss: ADC_DRIV.elf
	@echo 'Invoking: AVR Create Extended Listing'
	-avr-objdump -h -S ADC_DRIV.elf  >"ADC_DRIV.lss"
	@echo 'Finished building: $@'
	@echo ' '

ADC_DRIV.hex: ADC_DRIV.elf
	@echo 'Create Flash image (ihex format)'
	-avr-objcopy -R .eeprom -R .fuse -R .lock -R .signature -O ihex ADC_DRIV.elf  "ADC_DRIV.hex"
	@echo 'Finished building: $@'
	@echo ' '

sizedummy: ADC_DRIV.elf
	@echo 'Invoking: Print Size'
	-avr-size --format=avr --mcu=atmega32 ADC_DRIV.elf
	@echo 'Finished building: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) $(FLASH_IMAGE)$(ELFS)$(OBJS)$(ASM_DEPS)$(S_DEPS)$(SIZEDUMMY)$(S_UPPER_DEPS)$(LSS)$(C_DEPS) ADC_DRIV.elf
	-@echo ' '

secondary-outputs: $(LSS) $(FLASH_IMAGE) $(SIZEDUMMY)

.PHONY: all clean dependents

-include ../makefile.targets
